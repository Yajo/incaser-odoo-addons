<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>

    <record id="view_config_settings" model="ir.ui.view">
        <field name="name">Website Application</field>
        <field name="model">website.config.settings</field>
        <field name="inherit_id" ref="website.view_config_settings"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@name='google_analytics_key']" position="after">
                <label for="disable_website"/>
                <div name="disable_website">
                    <div>
                        <div name="disable_website" class="oe_inline">
                            <field name="disable_website"/>
                        </div>
                    </div>
                </div>
            </xpath>
        </field>
    </record>

</data>
</openerp>